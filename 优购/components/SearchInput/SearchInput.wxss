/* 搜索框基础的样式 */
.serach_input{
  height: 90rpx;
  padding:10rpx;
  background-color: var(--themeColor);
}
/* 超链接样式 */
.serach_input navigator{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  border-radius: 15rpx;
  color: #666;
}